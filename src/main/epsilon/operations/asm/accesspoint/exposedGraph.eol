// get annotation details entry for a given exposed graph
@cached
operation ASM!EModelElement getGraphAnnotation(graphFQName : String) : ASM!EStringToStringMapEntry {
    var annotation = asmUtils.getExtensionAnnotation(self, false);
    if (annotation.isPresent()) {
        return annotation.get().details.selectOne(d | d.key.startsWith(".graphs.") and d.value == accessPointFQName);
    } else {
        return null;
    }
}
