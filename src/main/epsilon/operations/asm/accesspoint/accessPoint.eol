// get annotation details entry for a given access point
@cached
operation ASM!EModelElement getAccessPointAnnotation(accessPointFQName : String) : ASM!EStringToStringMapEntry {
    var annotation = asmUtils.getExtensionAnnotation(self, false);
    if (annotation.isPresent()) {
        return annotation.get().details.selectOne(d | d.key.startsWith(".accessPoints.") and d.value == accessPointFQName);
    } else {
        return null;
    }
}
